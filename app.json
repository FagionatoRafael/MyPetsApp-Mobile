{
  "expo": {
    "name": "my-pets-app",
    "slug": "my-pets-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5CDB95"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {  
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-icon.png",
        "backgroundColor": "#5CDB95"
      },
      "googleServicesFile": "./google-services.json",
      "package": "br.com.myPetsApp"
    },
    "androidStatusBar": {
      "backgroundColor": "#5CDB95",
      "translucent": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo-icon.png",
          "color": "#5CDB95"
        }
      ]
    ],
    "notification": {
      "icon":"./assets/logo-icon.png",
      "color": "#5CDB95"
    }
  }
}
